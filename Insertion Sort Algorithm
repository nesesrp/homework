#include <iostream>



void insertionSort(int* A, int n);



int main()
{


    int A[6] = { 1, 2, 4, 5, 6, 3 };

    int n = sizeof(A) / sizeof(int);

    insertionSort(A, n);

    for (int i = 0; i < n; i++)
    {
    printf("%d,", A[i]);
    }
    
    getchar();

}

void insertionSort(int* A, int n)
{ 
    int key = 0;
    int i = 0;

    for (int j = 1; j < n; j++)
    {
        key = A[j];
        i = j - 1;

        while (i > 0 && A[i] > key)
        {
            A[i + 1] = A[i];
            i = i - 1;
        }
        A[i + 1] = key;
    }
}
